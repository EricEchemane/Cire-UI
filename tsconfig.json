{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": false,
    "strict": true,
    "forceConsistentCasingInFileNames": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": false,
    "outDir": "dist",
    "declaration": true,
    "esModuleInterop": true,
    "noEmit": false
  },
  "include": [
    // "next-env.d.ts",
    // "**/*.ts",
    // "**/*.tsx",
    "styled_components"
  ],
  "exclude": [
    "node_modules",
    "**/__tests__/*",
    "dist"
  ]
}